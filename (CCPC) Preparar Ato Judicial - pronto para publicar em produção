    <task-node end-tasks="true" name="(CCPC) Concluso para despacho de cumprimento de acórdão">
        <task name="(CCPC) Concluso para despacho de cumprimento de acórdão" swimlane="Assessoria">
            <controller>
                <variable name="minuta_ato_judicial" mapped-name="textEditCombo:minuta_ato_judicial" access="read,write"/>
                <variable name="MinutarEmLote" mapped-name="minutarLote:MinutarEmLote" access="read,write"/>
                <variable name="miniPAC" mapped-name="miniPAC:miniPAC" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - (CCPC) Preparar Ato Judicial" name="Nó de Desvio - (CCPC) Preparar Ato Judicial">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(CCPC) Assinar despacho de cumprimento de acórdão" name="Encaminhar para assinatura"/>
        <event type="task-create">
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavelTarefa('prepararAto:meiosComunicacoes','P,M,E,C')}"/>
        </event>
        <event type="node-leave">
            <action expression="#{not empty processoHome.idProcessoDocumento ? org.jboss.seam.bpm.processInstance.contextInstance.setVariable('minutaEmElaboracao', processoHome.idProcessoDocumento) : ''}"/>
        </event>
        <event type="task-end">
            <action expression="#{tramitacaoProcessualService.gravaVariavel('minutaEmElaboracao', tramitacaoProcessualService.recuperaVariavelTarefa('textEditCombo:minuta_ato_judicial'))}"/>
        </event>
        <event type="task-start">
            <action name="Condicao do lancamento temporario de movimentos" expression="#{lancadorMovimentosService.setCondicaoLancamentoMovimentosTemporarioNoFluxo('#{true}')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavelTarefa('pje:fluxo:documento:tipo:minuta:ids','63,64')}"/>
        </event>
    </task-node>
    <task-node end-tasks="true" name="(CCPC) Assinar despacho de cumprimento de acórdão">
        <task name="(CCPC) Assinar despacho de cumprimento de acórdão" swimlane="Magistrado">
            <controller>
                <variable name="Processo_Fluxo_revisarMinuta" mapped-name="frame:Processo_Fluxo_revisarMinuta" access="read,write"/>
                <variable name="MovimentarEmLote" mapped-name="movimentarLote:MovimentarEmLote" access="read,write"/>
                <variable name="AssinaturaEmLote" mapped-name="assinarLote:AssinaturaEmLote" access="read,write"/>
                <variable name="HomologarMovimentos" mapped-name="homologadorEventoTree:HomologarMovimentos" access="read"/>
                <variable name="miniPAC" mapped-name="miniPAC:miniPAC" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - (CCPC) Preparar Ato Judicial" name="Nó de Desvio - (CCPC) Preparar Ato Judicial">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(CCPC) Concluso para despacho de cumprimento de acórdão" name="Retornar para minuta"/>
        <transition to="(CCPC) Testa Expediente Criado" name="(CCPC) Testa Expediente Criado">
            <condition expression="#{true}"/>
        </transition>
        <event type="task-create">
            <action name="upd" expression="#{taskInstanceUtil.setFrameDefaultTransition('(CCPC) Testa Expediente Criado')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavelTarefa('prepararAto:meiosComunicacoes','P,M,E,C')}"/>
        </event>
        <event type="task-start">
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavelTarefa('Processo_Fluxo_revisarMinuta','63,64')}"/>
            <action name="upd" expression="#{lancadorMovimentosService.setCondicaoLancamentoMovimentosTemporarioNoFluxo('#{false}')}"/>
        </event>
    </task-node>
    <decision expression="#{!empty tramitacaoProcessualService.recuperaVariavel('comunicacaoProcessualAction:idsExpedientes') ? 'PAJ - Grava Variável de Expediente Gerado' : '(CCPC) Apaga variáveis usadas'}" name="(CCPC) Testa Expediente Criado">
        <transition to="PAJ - Grava Variável de Expediente Gerado" name="PAJ - Grava Variável de Expediente Gerado"/>
        <transition to="(CCPC) Apaga variáveis usadas" name="(CCPC) Apaga variáveis usadas"/>
    </decision>
    <node name="PAJ - Grava Variável de Expediente Gerado">
        <transition to="PAJ - Desloca Processo para Fluxo do PAC" name="PAJ - Desloca Processo para Fluxo do PAC"/>
        <event type="node-leave">
            <action expression="#{tramitacaoProcessualService.gravaVariavel(&quot;pje:miniPac:isExpedientesMinuta&quot;,'true')}"/>
        </event>
    </node>
    <node name="PAJ - Desloca Processo para Fluxo do PAC">
        <transition to="(CCPC) Altera variáveis miniPac" name="(CCPC) Altera variáveis miniPac"/>
        <event type="node-leave">
            <action expression="#{processoJudicialService.incluirNovoFluxo(tramitacaoProcessualService.recuperaProcesso(),'PAC',org.jboss.seam.bpm.processInstance.getContextInstance().getVariables())}"/>
        </event>
    </node>
    <node name="(CCPC) Apaga variáveis usadas">
        <transition to="Término" name="Término"/>
        <event type="node-leave">
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_urgencia_inicial')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_urgencia')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_generica')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_agravo_regimental')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_agravo_interno')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_extincao')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_reativacao')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:decisao_embargos_declaracao')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:despacho')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:despacho_inicial')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:despacho_generico')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.apagaVariavel('ccpc:paj:despacho_cumprimento_acordao')}"/>
            <action expression="#{tramitacaoProcessualService.gravaVariavel(&quot;pje:miniPac:isExpedientesMinuta&quot;,'false')}"/>
        </event>
    </node>
    <node name="(CCPC) Altera variáveis miniPac">
        <transition to="(CCPC) Aguardar retorno de expedientes" name="(CCPC) Aguardar retorno de expedientes"/>
        <event type="node-leave">
            <action expression="#{tramitacaoProcessualService.gravaVariavel(&quot;pje:miniPac:isExpedientesMinuta&quot;,'false')}"/>
        </event>
    </node>
    <process-state name="(CCPC) Aguardar retorno de expedientes">
        <sub-process name="Controle de prazos" binding="late"/>
        <transition to="Término" name="Término"/>
    </process-state>
    <end-state name="Término">
        <event type="node-enter">
            <action expression="#{org.jboss.seam.bpm.processInstance.contextInstance.deleteVariable('minutaEmElaboracao')}"/>
            <action expression="#{org.jboss.seam.bpm.processInstance.contextInstance.deleteVariable('MovimentosLancadosTemporariamente')}"/>
            <action expression="#{org.jboss.seam.bpm.processInstance.contextInstance.deleteVariable('AgrupamentosLancadosTemporariamente')}"/>
            <action expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_urgencia_inicial','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_urgencia','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_generica','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_agravo_regimental','false')}"/>
            <action expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_agravo_interno','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_extincao','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_reativacao','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:decisao_embargos_declaracao','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:despacho','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:despacho_inicial','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:despacho_generico','false')}"/>
            <action name="upd" expression="#{tramitacaoProcessualService.gravaVariavel('ccpc:paj:despacho_cumprimento_acordao','false')}"/>
        </event>
    </end-state>  
    <!-- PROCESS-EVENTS -->
